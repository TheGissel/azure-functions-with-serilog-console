{
    "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "None",
      "Microsoft.Azure.WebJobs.Hosting": "Warning"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}